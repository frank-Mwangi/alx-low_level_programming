CC = gcc
SRC = main.c school.c
NAME = school
CFLAGS = -Wall -Werror -Wextra -pedantic

.PHONY: all clean oclean fclean re

clean: 
	${RM} *~ ${NAME}
all: m.h main.o school.o
	${CC} -Wall -Werror -Wextra -pedantic main.o school.o -o ${NAME}
oclean:
	${RM} main.o school.o
fclean: clean oclean
re: fclean all
